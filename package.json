{
  "name": "terminal-everywhere",
  "description": "A simple framework for building shell and browser command line apps",
  "version": "1.2.0",
  "keywords": ["util", "functional", "browser", "terminal", "console", "shell"],
  "homepage": "http://github.com/fiatjaf/terminal",
  "author": "Giovanni Torres Parra <fiatjaf@gmail.com>",
  "maintainers": [{
    "name": "Giovanni Torres Parra",
    "email": "fiatjaf@gmail.com"
  }],
  "licenses": [{ "type": "MIT", "url": "https://raw.github.com/fiatjaf/terminal/master/LICENSE" }],
  "repository": { "type": "git", "url": "http://github.com/fiatjaf/terminal" },
  "scripts": {
    "build": "./node_modules/.bin/browserify --standalone Terminal -t coffeeify -t brfs --extension='.coffee' index.coffee -d --outfile dist/terminal.js"
  },
  "directories": {
    "lib": "lib"
  },
  "browser": {
    "less": "./lib/less.js"
  },
  "browserify-shim": {
    "less": "less"
  },
  "dependencies": {
    
  },
  "devDependencies": {
    "coffeeify": "0.6.0",
    "browserify": "2.35.4",
    "browserify-shim": ">= 3.0.0",
    "mime": "1.2.x",
    "insert-css": ">= 0.1.1",
    "brfs": "1.0.0"
  }
}
